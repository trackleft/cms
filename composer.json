{
    "name": "trackleft/cms",
    "description": "An install profile for shared configuration management with Drupal CMS",
    "type": "drupal-custom-profile",
    "require": {
        "drupal/config_distro": "dev-2.0.x",
        "drupal/config_ignore": "dev-3.x",
        "drupal/config_ignore_readonly": "dev-2.x",
        "drupal/config_provider": "dev-3.0.x",
        "drupal/config_readonly": "dev-1.x",
        "drupal/config_snapshot": "dev-2.0.x",
        "drupal/config_split": "dev-2.0.x",
        "drupal/config_sync": "dev-3.0.x",
        "drupal/config_update": "dev-2.0.x",
        "drupal/core": "^11"
    },
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Green",
            "homepage": "https://github.com/trackleft"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "patches": {
            "drupal/config_distro": {
                "Allow to work with Drupal 11 (3428536)": "https://git.drupalcode.org/project/config_distro/-/merge_requests/23.diff"
            },
            "drupal/config_sync": {
                "Allow to work with Drupal 11 (3429477)": "https://git.drupalcode.org/project/config_sync/-/merge_requests/22.diff"
            }
        }
    }
}
